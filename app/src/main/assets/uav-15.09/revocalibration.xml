<xml>
    <object name="RevoCalibration" category="Sensors" settings="true" singleinstance="true">
        <description>Settings for the INS to control the algorithm and what is updated</description>
        <field name="mag_bias" defaultvalue="0,0,0" elementnames="X,Y,Z" type="float" units="mGau"/>
        <field name="mag_transform" defaultvalue="1,0,0,0,1,0,0,0,1" elementnames="r0c0,r0c1,r0c2,r1c0,r1c1,r1c2,r2c0,r2c1,r2c2" type="float"
         units="gain"/>
        <!-- These settings are related to how the sensors are post processed -->
        <!-- TODO: reimplement, put elsewhere (later) -->
        <field name="BiasCorrectedRaw" defaultvalue="TRUE" elements="1" options="FALSE,TRUE" type="enum" units=""/>
        <field name="MagBiasNullingRate" defaultvalue="0" elements="1" type="float" units=""/>
        <access flight="readwrite" gcs="readwrite"/>
        <telemetrygcs acked="true" period="0" updatemode="onchange"/>
        <telemetryflight acked="true" period="0" updatemode="onchange"/>
        <logging period="0" updatemode="manual"/>
    </object>
</xml>
